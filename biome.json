{
  "$schema": "./node_modules/@biomejs/biome/configuration_schema.json",
  "files": {
    "ignoreUnknown": false,
    "includes": ["**", "!**/dist", "!**/.zed", "!**/.vscode", "!bts.jsonc"]
  },
  "linter": {
    "rules": {
      "style": {
        "noMagicNumbers": "warn"
      },
      "suspicious": {
        "noApproximativeNumericConstant": "warn",
        "noConsole": "warn"
      }
    }
  },
  "extends": ["ultracite"]
}
